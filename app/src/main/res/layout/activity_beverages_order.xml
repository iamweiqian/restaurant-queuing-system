<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="wqyap762.rprqs.BeveragesOrderActivity">

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridLayout7"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <Button
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:text="Ice Lemon Tea"
            android:id="@+id/button19"
            android:layout_row="0"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:id="@+id/button20"
            android:layout_row="1"
            android:layout_column="0"
            android:text="Green Tea" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="100 PLUS"
            android:id="@+id/button21"
            android:layout_row="2"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:id="@+id/button22"
            android:layout_row="3"
            android:layout_column="0"
            android:text="Coca-Cola" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Chinese Tea"
            android:id="@+id/button23"
            android:layout_row="4"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Sky Juice"
            android:id="@+id/button24"
            android:layout_row="5"
            android:layout_column="0" />
    </GridLayout>
</RelativeLayout>
