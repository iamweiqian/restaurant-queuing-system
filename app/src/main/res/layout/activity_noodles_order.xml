<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="wqyap762.rprqs.NoodlesOrderActivity">

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridLayout6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <Button
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:text="WA TAN HO"
            android:id="@+id/waTanHoButton"
            android:layout_row="0"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Singapore Fried Bee Hoon"
            android:id="@+id/singaporeFriedBeeHoonButton"
            android:layout_row="1"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Hokkien Mee"
            android:id="@+id/hokkienMeeButton"
            android:layout_row="2"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Singapore Fried Hokkien Mee"
            android:id="@+id/singaporeFriedHokkienMeeButton"
            android:layout_row="3"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Claypot Yee Mee"
            android:id="@+id/claypotYeeMeeButton"
            android:layout_row="4"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Tomato Mee"
            android:id="@+id/tomatoMeeButton"
            android:layout_row="5"
            android:layout_column="0" />
    </GridLayout>
</RelativeLayout>
