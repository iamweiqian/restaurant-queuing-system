<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="wqyap762.rprqs.NoodlesOrderActivity">

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridLayout6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <Button
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:text="Dumpling Noodles"
            android:id="@+id/dumplingNoodlesButton"
            android:layout_row="0"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Wanton Noodles"
            android:id="@+id/wanTonNoodlesButton"
            android:layout_row="1"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Fried Fish Bee Hoon Soup"
            android:id="@+id/friedFishBeeHoonSoupButton"
            android:layout_row="2"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Sliced Fish Bee Hoon Soup"
            android:id="@+id/slicedFishBeeHoonSoupButton"
            android:layout_row="3"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Sin Chew Bee Hoon"
            android:id="@+id/sinChewBeeHoonButton"
            android:layout_row="4"
            android:layout_column="0" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Ee Mee"
            android:id="@+id/eeMeeButton"
            android:layout_row="5"
            android:layout_column="0" />
    </GridLayout>
</RelativeLayout>
