<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wqyap762.rprqs.SetOrderActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar" />

    <GridLayout
        android:id="@+id/gridLayout4"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/app_bar"
        android:layout_centerHorizontal="true"
        android:layout_margin="16dp">

        <Button
            android:id="@+id/setAButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="0"
            android:text="Set A" />

        <Button
            android:id="@+id/setBButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="1"
            android:text="Set B" />

        <Button
            android:id="@+id/setCButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="2"
            android:text="Set C" />

        <Button
            android:id="@+id/setDButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="3"
            android:text="Set D" />

        <Button
            android:id="@+id/setEButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="4"
            android:text="Set E" />

        <Button
            android:id="@+id/setFButton"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_column="0"
            android:layout_row="5"
            android:text="Set F" />
    </GridLayout>

</RelativeLayout>
